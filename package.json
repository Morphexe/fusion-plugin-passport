  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-eslint": "8.2.6",
    "create-universal-package": "3.4.6",
    "eslint": "5.3.0",
    "eslint-config-fusion": "^4.0.0",
